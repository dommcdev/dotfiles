[credential]
    helper = store --file=$HOME/.config/git/credentials
[user]
	email = dominicmcdev@gmail.com
	name = Dominic
[core]
	excludesfile = $HOME/.config/git/ignore
[init]
	defaultBranch = main
[alias]
    lg = log --graph --abbrev-commit --decorate --all --color \
--pretty=format:'%C(bold red)%h%C(reset) - %C(bold cyan)%ad%C(reset) %C(bold yellow)(%ar)%C(reset) %C(bold magenta)%d%C(reset) %s - %C(bold green)%an%C(reset)' \
--date=format:'%Y-%m-%d %H:%M'
