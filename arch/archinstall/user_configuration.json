        "sys_lang": "en_US.UTF-8"
    },
    "mirror_config": {
        "custom_repositories": [],
        "custom_servers": [],
        "mirror_regions": {},
        "optional_repositories": []
    },
    "network_config": {
        "type": "nm"
    },
    "ntp": true,
    "packages": ["git"],
    "parallel_downloads": 5,
    "profile_config": {
        "gfx_driver": null,
        "greeter": null,
