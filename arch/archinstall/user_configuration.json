    "custom-commands": [
        "su -l dominic -c 'mkdir -p /home/dominic/dev'",
        "su -l dominic -c 'git clone http://developermcd.com:8080/dominic/dotfiles.git /home/dominic/dev/dotfiles'" 
    ],
    "hostname": "kabigon",
